{
  "compilerOptions": {
    "types": ["node", "koa"],
    "typeRoots": ["node_modules/@types"],
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "service",
    "allowJs": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": ["typings.d.ts", "./**/*"],
  "exclude": ["node_modules"],
  "compileOnSave": true
}