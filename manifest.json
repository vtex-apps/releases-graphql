    {
      "name": "outbound-access",
      "attrs": {
        "host": "delorean.vtex.com",
        "path": "/publications/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "rlsnts-env-stable.us-east-1.elasticbeanstalk.com",
        "path": "/releases/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "vtexid.vtex.com.br",
        "path": "/api/*"
      }
    }
  ]
