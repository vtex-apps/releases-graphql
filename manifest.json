{
  "name": "releases-graphql",
  "vendor": "vtex",
  "version": "0.1.0",
  "title": "Releases Graphql",
  "description": "VTEX Releases GraphQL server",
  "mustUpdateAt": "2019-08-09",
  "categories": [],
  "registries": [
    "smartcheckout"
  ],
  "settingsSchema": {},
  "dependencies": {},
  "builders": {
    "graphql": "1.x",
    "node": "3.x"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "vbase-read-write"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "koto.vtex.com",
